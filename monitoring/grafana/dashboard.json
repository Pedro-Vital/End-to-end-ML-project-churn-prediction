      "id": 10,
      "options": {
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        }
      },
      "pluginVersion": "10.0.0",
      "targets": [
        { "expr": "model_loaded", "refId": "A" }
      ],
      "title": "ML Model Loaded",
      "type": "stat"
    },

    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "gridPos": { "h": 5, "w": 12, "x": 12, "y": 0 },
      "id": 2,
      "targets": [
        {
          "expr": "rate(http_requests_total{job=\"fastapi\"}[1m])",
          "legendFormat": "{{method}} {{handler}}",
          "refId": "A"
        }
      ],
      "title": "HTTP Request Throughput (req/s)",
      "type": "timeseries"
    },

    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 5 },
      "id": 3,
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_highr_seconds_bucket{job=\"fastapi\"}[5m])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_highr_seconds_bucket{job=\"fastapi\"}[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_highr_seconds_bucket{job=\"fastapi\"}[5m])) by (le))",
          "legendFormat": "p99",
          "refId": "C"
        }
